{
  "name": "galaxy-classification-app",
  "private": true,
  "version": "0.1.2",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:frontend:host": "vite --host 0.0.0.0",
    "dev:backend": "convex dev",
    "dev:images": "dotenv -e .env.local -- node scripts/local-image-server.mjs",
    "dev:all": "npm-run-all --parallel dev:frontend dev:backend dev:images",
    "dev:all:hosts": "npm-run-all --parallel dev:frontend:host dev:backend dev:images",
    "build": "vite build",
    "lint": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build"
  },
  "dependencies": {
    "@convex-dev/aggregate": "^0.1.23",
    "@convex-dev/auth": "^0.0.80",
    "@react-email/render": "^1.2.2",
    "clsx": "^2.1.1",
    "convex": "^1.24.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-email": "^4.2.8",
    "react-router": "^7.8.2",
    "resend": "^6.0.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "~10",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "~8",
    "prettier": "^3.5.3",
    "tailwindcss": "~3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
